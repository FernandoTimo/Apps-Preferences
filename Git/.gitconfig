[core]
	editor = code --wait
[user]
	email = fertimo99@gmail.com
	name = FernandoTimo
[alias]
	a = add .
	s = status
	c = "!f() { git commit -m \"$*\"; }; f"
	ac = "!f() { git a && git c \"$*\"; }; f"
	ch = checkout -- . 
	cch = rm -r --cached 
	b = branch
	bs = branch -M main 
	r = restore .
	rr = reset --soft HEAD~
	ra = "!f() { git remote add origin git@github-\"$1\":\"$1\"/\"$2\".git; };f"
	rs = "!f() { git remote set-url origin git@github-\"$1\":\"$1\"/\"$2\".git ; };f"
	p = push
	pf = push -f origin main
	pu = push -u origin main
	last = log -1 HEAD
	file = config --global -e
	all = "!f() { git commit -am \"$*\" && git p; }; f"
	show = "!gitk"
	k = "!f(){ rm -rf .git; };f"
	link = "!f(){ rm ~/.gitconfig && cp -rl Git/.gitconfig ~/; };f"
	un = config --global user.name 
	um = config --global user.email 
	l = "!git config --local user.name && git config --local user.email"
	g = "!git config --global user.name && git config --global user.email"
	lt = "!git config --local user.name Timoideas && git config --local user.email Timoideas@hotmail.com"
	gt = "!git config --global user.name Timoideas && git config --global user.email Timoideas@hotmail.com"
	lf = "!git config --local user.name FernandoTimo && git config --local user.email fertimo99@gmail.com"
	gf = "!git config --global user.name FernandoTimo && git config --global user.email fertimo99@gmail.com"
	nf = clone https://github.com/FernandoTimo/Frontend-Next Frontend
	nb = clone https://github.com/FernandoTimo/Backend-Express Backend
	na = "!git clone https://github.com/FernandoTimo/Frontend-Next Frontend && git clone https://github.com/FernandoTimo/Backend-Express Backend"
	e = "!f(){ echo \"#$1\" >> README.md && echo \"desktop.ini\" >> .gitignore }f"
	i = "!f() { ([ !$2 ] && ( echo \"Second parameter, repository name expected. \")) || ([ $1 == 'T' ] && ( git init && git lt && git e $1 && git ac Inicio && git bs && git remote add origin git@github-\"$1\":\"$1\"/\"$2\".git && git pu;)) || ([ $1 == 'F' ] && ( git init && git lf && git e $1 && git add . && git commit -m Inicio && git bs && git remote add origin git@github-\"$1\":\"$1\"/\"$2\".git && git pu;)) || (echo \"Invalid value, try: T, F \") }; f"	
	
	nfp = "!f() { ([ !$2 ] && ( echo \"Second parameter, repository name expected. \")) || ([ $1 == 'T' ] && ( git e $1 && git init && git config --local user.name Timoideas && git config --local user.email Timoideas@hotmail.com && git clone https://github.com/FernandoTimo/Frontend-Next Frontend && git clone https://github.com/FernandoTimo/Backend-Express Backend && git add . && git commit -m Inicio && git bs && git remote add origin git@github-\"$1\":\"$1\"/\"$2\".git && git pu))  || ( ([ $1 == 'F' ] && git e $1 && git init && git config --local user.name FernandoTimo && git config --local user.email fertimo99@gmail.com && git clone https://github.com/FernandoTimo/Frontend-Next Frontend && git clone https://github.com/FernandoTimo/Backend-Express Backend && git add . && git commit -m Inicio && git bs && git remote add origin git@github-\"$1\":\"$1\"/\"$2\".git && git pu;)); } ; f"