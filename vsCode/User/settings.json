  "vscode-graphql.useSchemaFileDefinitions": false,
  "color-highlight.languages": ["*"],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": { "editor.autoClosingBrackets": "always" },
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "python.terminal.executeInFileDir": true,
  "python.formatting.provider": "autopep8",

  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.tabSize": 2
  },
  "workbench.colorCustomizations": {
    "[Timoideas - Esmeralda Soft]": {
      "tab.activeBackground": "#16463e",
      "tab.activeForeground": "#ffc400",
      "tab.hoverBackground": "#0f2c27"
    }
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "foreground": "#8c8c8c"
        }
      }
    ]
  },
  "vscodeGoogleTranslate.preferredLanguage": "Spanish",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "workbench.statusBar.visible": false,
  "polacode.shadow": "rgba(0, 0, 0, 0.55) 0px 20px 68px",
  "polacode.transparentBackground": true,
  "colorize.languages": ["typescript", "javascript", "css", "scss"],
  "colorize.include": [
    "**/*.css",
    "**/*.scss",
    "**/*.sass",
    "**/*.less",
    "**/*.styl",
    "**/*.js"
  ],
  "editor.inlineSuggest.enabled": true,
  "bracket-pair-colorizer-2.depreciation-notice": false
}
